{
    "name": "fastwf/form",
    "description": "Form handler library",
    "type": "library",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Fastwf/api"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Fastwf/constraint"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fastwf\\Form\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fastwf\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "David BELBEZE",
            "email": "david.belbeze@gmail.com"
        }
    ],
    "require": {
        "fastwf/constraint": "dev-master",
        "psr/http-message": "^1.0",
        "fastwf/api": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/psr7": "^2.1"
    }
}
